{
  "name": "simple-cookie-util",
  "version": "1.0.0",
  "description": "Simple Cookie utility for browsers",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "prepare": "husky install",
    "lint": "eslint src --ext .ts,.js --no-cache",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "src/**/*.{ts,js,json,css,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gmaye/simple-cookie-util.git"
  },
  "keywords": [
    "cookie"
  ],
  "author": "gmaye",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gmaye/simple-cookie-util/issues"
  },
  "homepage": "https://github.com/gmaye/simple-cookie-util#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-modules": "^0.1.6",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.13"
  }
}
